<h1> Cats </h1>
<% @cats.each do |cat| %>
  <tr>
    <td>Name: <%= cat.name%></td>
  </BR>
  </tr>
<% end %>
<h1> Todos </h1>
<% @todos.each do |todo| %>
  <tr>
    <td>Tasks: <%= todo.tasks%></td>
    <td>Finished: <%= todo.finished%></td>

  </BR>
  </tr>
<% end %>
<h1> Users </h1>
<% @users.each do |user| %>
  <tr>
    <td>Username: <%= user.username%></td>
    <td>Email: <%= user.email%></td>
    <td>Age: <%= user.age%></td>

  </BR>
  </tr>
<% end %>
